            },
            {
              "id": "c01b75e9-6bc3-4784-93e4-c6b204b5b2d0",
              "type": "basic.code",
              "data": {
                "ports": {
                  "in": [
                    {
                      "name": "clk"
                    },
                    {
                      "name": "ps2_code",
                      "range": "[7:0]",
                      "size": 8
                    },
                    {
                      "name": "ps2_valid"
                    },
                    {
                      "name": "shift"
                    }
                  ],
                  "out": [
                    {
                      "name": "ascii",
                      "range": "[7:0]",
                      "size": 8
                    },
                    {
                      "name": "ascii_valid"
                    }
                  ]
                },
                "params": [],
                "code": "/*                       PS/2 decoder to ASCII code (English keyboard)\r\nSources consulted:\r\n* https://groups.google.com/g/fpga-wars-explorando-el-lado-libre/c/ysO-TZn8sEo/m/jZxRR-KRBgAJ\r\n* https://github.com/Obijuan/PS2-KeyBoard-FPGA/tree/master/ps2-keyb-collection/examples\r\n* https://www.ele.uva.es/~jesus/\r\n  ---> source: https://www.ele.uva.es/~jesus/VZ200.tgz (keyb.v)\r\n* https://www.eecg.utoronto.ca/~pc/courses/241/DE1_SoC_cores/ps2/ps2.html\r\n* LLMs\r\n*/\r\n\r\n    // Register to store the conversion result\r\n    reg [7:0] ascii_reg = 0;\r\n    reg       valid_reg = 0;\r\n\r\n    always @(posedge clk)  begin\r\n            if (ps2_valid) begin\r\n                    valid_reg <= 1;\r\n                    \r\n                    case (ps2_code)\r\n                        // Numbers and special characters\r\n                        'h16: ascii_reg <= shift ? \"!\"  : \"1\";\r\n                        'h1E: ascii_reg <= shift ? 'h22 : \"2\"; // Double quotes\r\n                        'h26: ascii_reg <= shift ? 'hFA : \"3\"; // Midpoint?\r\n                        'h25: ascii_reg <= shift ? \"$\"  : \"4\";\r\n                        'h2E: ascii_reg <= shift ? \"%\"  : \"5\";\r\n                        'h36: ascii_reg <= shift ? \"&\"  : \"6\";\r\n                        'h3D: ascii_reg <= shift ? \"/\"  : \"7\";\r\n                        'h3E: ascii_reg <= shift ? \"(\"  : \"8\";\r\n                        'h46: ascii_reg <= shift ? \")\"  : \"9\";\r\n                        'h45: ascii_reg <= shift ? \"=\"  : \"0\";\r\n                        'h4E: ascii_reg <= shift ? \"?\"  : \"+\";\r\n                        'h41: ascii_reg <= shift ? \";\"  : \",\";\r\n                        'h49: ascii_reg <= shift ? \":\"  : \".\";\r\n                        'h4C: ascii_reg <= shift ? \"_\"  : \"-\";\r\n                        'h3C: ascii_reg <= shift ? \"*\"  : \"'\";\r\n                        // Letters (uppercase with Shift)\r\n                        'h1C: ascii_reg <= shift ? \"A\" : \"a\";\r\n                        'h32: ascii_reg <= shift ? \"B\" : \"b\";\r\n                        'h21: ascii_reg <= shift ? \"C\" : \"c\";\r\n                        'h23: ascii_reg <= shift ? \"D\" : \"d\";\r\n                        'h24: ascii_reg <= shift ? \"E\" : \"e\";\r\n                        'h2B: ascii_reg <= shift ? \"F\" : \"f\";\r\n                        'h34: ascii_reg <= shift ? \"G\" : \"g\";\r\n                        'h33: ascii_reg <= shift ? \"H\" : \"h\";\r\n                        'h43: ascii_reg <= shift ? \"I\" : \"i\";\r\n                        'h3B: ascii_reg <= shift ? \"J\" : \"j\";\r\n                        'h42: ascii_reg <= shift ? \"K\" : \"k\";\r\n                        'h4B: ascii_reg <= shift ? \"L\" : \"l\";\r\n                        'h3A: ascii_reg <= shift ? \"M\" : \"m\";\r\n                        'h31: ascii_reg <= shift ? \"N\" : \"n\";\r\n                        'h44: ascii_reg <= shift ? \"O\" : \"o\";\r\n                        'h4D: ascii_reg <= shift ? \"P\" : \"p\";\r\n                        'h15: ascii_reg <= shift ? \"Q\" : \"q\";\r\n                        'h2D: ascii_reg <= shift ? \"R\" : \"r\";\r\n                        'h1B: ascii_reg <= shift ? \"S\" : \"s\";\r\n                        'h2C: ascii_reg <= shift ? \"T\" : \"t\";\r\n                        'h3C: ascii_reg <= shift ? \"U\" : \"u\";\r\n                        'h2A: ascii_reg <= shift ? \"V\" : \"v\";\r\n                        'h1D: ascii_reg <= shift ? \"W\" : \"w\";\r\n                        'h22: ascii_reg <= shift ? \"X\" : \"x\";\r\n                        'h35: ascii_reg <= shift ? \"Y\" : \"y\";\r\n                        'h1A: ascii_reg <= shift ? \"Z\" : \"z\";\r\n                        // Other characters\r\n                        'h29:    ascii_reg  <=  \" \"; // Space\r\n                        'h5A:    ascii_reg  <= 'h0A; // Enter (LF)\r\n                        default: ascii_reg  <= 'h00; // Unrecognized code\r\n                    endcase\r\n            end else valid_reg <= 0;\r\n    end\r\n\r\n    // Assign outputs\r\n    assign ascii       = ascii_reg;\r\n    assign ascii_valid = valid_reg;\r\n    "
              },
              "position": {
